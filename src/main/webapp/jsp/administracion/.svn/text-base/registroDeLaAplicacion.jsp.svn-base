<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<%@ page pageEncoding="UTF-8" %>
<%@ taglib prefix="fmt" uri="/WEB-INF/tld/fmt.tld" %>
<%@ taglib prefix="c" uri="/WEB-INF/tld/c.tld" %>
<div dojoType="dijit.Dialog" id="formularioPopUp" class="popUp" title="<fmt:message key="timeLapse.jsp.container.registro.titulo"/>" >
	<form onsubmit="return false" dojoType="dijit.form.Form"  
		encType="multipart/form-data" action="" method="">		
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputNombreUsuario"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formNombreUsu" 
					dojoType="dijit.form.ValidationTextBox"
					required="true"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipNombreUsuario"/>"
					invalidMessage ="<fmt:message key="timeLapse.jsp.container.registro.advertenciaNombreUsuario"/>"
					type="text"
					maxLength="50"
					name="formNombreUsu"/>
			</span>
		</div>
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputNombrePropio"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formNombrePropio" 
					dojoType="dijit.form.ValidationTextBox"
					required="true"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipNombrePropio"/>"
					invalidMessage ="<fmt:message key="timeLapse.jsp.container.registro.advertenciaNombrePropio"/>"
					type="text"
					maxLength="100"
					name="formNombrePropio"/>
			</span>
		</div>
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputPrimerApellido"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formApellido1" 
					dojoType="dijit.form.ValidationTextBox"
					required="true"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipPrimerApellido"/>"
					invalidMessage ="<fmt:message key="timeLapse.jsp.container.registro.advertenciaPrimerApellido"/>"
					type="text"
					maxLength="100"
					name="formApellido1"/>
			</span>
		</div>
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputSegundoApellido"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formApellido2" 
					dojoType="dijit.form.ValidationTextBox"
					required="false"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipSegundoApellido"/>"
					type="text"
					maxLength="100"
					name="formApellido2"/>
			</span>
		</div>
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputPassword1"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formPasswd1" 
					dojoType="dijit.form.ValidationTextBox"
					required="true"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipPassword1"/>"
					invalidMessage ="<fmt:message key="timeLapse.jsp.container.registro.advertenciaPassword1"/>"
					type="password"
					maxLength="100"
					name="formPasswd1"/>
			</span>
		</div>
		<div class="elemFormulario">
			<span class="cajaTexto">
				<span class="labelTexto"><fmt:message key="timeLapse.jsp.container.registro.inputPassword2"/></span>
			</span>
			<span class="cajaInputGran">
				<input id="formPasswd2" 
					dojoType="dijit.form.ValidationTextBox"
					required="true"
					trim="true"  
					promptMessage="<fmt:message key="timeLapse.jsp.container.registro.tooltipPassword1"/>"
					invalidMessage ="<fmt:message key="timeLapse.jsp.container.registro.advertenciaPassword1"/>"
					type="password"
					maxLength="100"
					name="formPasswd2"/>
			</span>
		</div>
		
		
		
		
		<div class="contenedorBotones" id="botonesPopUp">
			<div dojoType="dijit.form.Button" id="botonGuardar" label="<fmt:message key="timeLapse.jsp.botonGuardar" />"></div>	
		</div>
	</form>


</div>




